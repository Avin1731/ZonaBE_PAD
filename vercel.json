{
    "version": 2,
    "framework": null,
    "functions": {
        "public/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Credentials", "value": "true" },
                { "key": "Access-Control-Allow-Origin", "value": "https://zona-fe-pad.vercel.app" },
                { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
                { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(css|js|images|storage|build|assets)/(.*)",
            "destination": "/public/$1/$2"
        },
        {
            "source": "/(.*)",
            "destination": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://zona-be-pad.vercel.app",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}